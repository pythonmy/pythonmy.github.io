{{#each companies }}
    <div class="col-lg-3">
        <p class="well">
            <img src="{{ this.logo }}" alt="{{ this.name }}" height="{{ this.height }}" width="{{ this.width }}"/>
            <br />
            <a href="{{ this.url }}" target="_blank">{{ this.name }}</a>
        </p>
    </div>
{{/each}}
